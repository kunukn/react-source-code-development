{
  "name": "@kunukn/react-source-code-development",
  "version": "0.0.1",
  "description": "Source code development for the ReactJS project",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --color --mode development --env.PORT=6009",
    "dev": "yarn start",
    "build": "webpack --mode production",
    "use-from-react-command-line-1": "yarn build react/index,react-dom/index --type=UMD_DEV",
    "use-from-react-command-line-2": "yarn build react/index,react-dom/index --type=UMD_PROD",
    "use-from-react-command-line-3": "yarn build react/index,react-dom/index --type=NODE_DEV",
    "use-from-react-command-line-4": "yarn build react/index,react-dom/index --type=NODE_PROD",
    "use-from-react-command-line-5": "yarn build react-dom/index --type=UMD_DEV",
    "use-from-react-command-line-6": "yarn build react/index,react-dom/index --type=UMD",
    "use-from-react-command-line-test-file": "cross-env NODE_ENV=development jest scripts/babel/__tests__/my-new-test-file-name.js --config ./scripts/jest/config.source.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@reach/component-component": "^0.9.0",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "file-loader": "^5.1.0",
    "flow-bin": "^0.120.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-md5-hash": "0.0.6"
  },
